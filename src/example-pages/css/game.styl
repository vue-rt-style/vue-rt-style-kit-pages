.ld-mygame-line
  position: relative;
  &:before
    content ''
    position: absolute;
    top: 0
    left: 0
    height: 2px
    height: 2px
    width: 90vw
    background-image: linear-gradient(to right , rgba(48,121,215,1) 0%, rgba(48,121,215,0) 100%)
