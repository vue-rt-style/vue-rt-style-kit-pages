.ld-mygame
  overflow-x hidden
.ld-mygame,
.F
  background-color: #0f172c!important
  .app-header
    display: none
.ld-mygame-users

  background-image: url(https://static-cdn.rt.ru/sites/default/files/img/mygame/users-bg.jpg)
  background-size: cover;
  transition-timing-function ease-in-out
  transition-duration 0.5s
  transition-property background
  &>.rt-container
    @media(max-width: 1024px)
      padding-left: 0px
      padding-right: 0px

  @media (-webkit-min-device-pixel-ratio: 2), (min-resolution: 192dpi) {
    background-image: url(https://static-cdn.rt.ru/sites/default/files/img/mygame/users-bg-2x.jpg)
  }
.ld-mygame-users--warface
  background-image: url(https://static-cdn.rt.ru/sites/default/files/img/mygame/warface-bg.jpg)
  @media (-webkit-min-device-pixel-ratio: 2), (min-resolution: 192dpi) {
    background-image: url(https://static-cdn.rt.ru/sites/default/files/img/mygame/warface-bg-2x.jpg)
  }
.ld-mygame-users--archeage
  background-image: url(https://static-cdn.rt.ru/sites/default/files/img/mygame/archeage-bg.jpg)
  @media (-webkit-min-device-pixel-ratio: 2), (min-resolution: 192dpi) {
    background-image: url(https://static-cdn.rt.ru/sites/default/files/img/mygame/archeage-bg-2x.jpg)
  }
.ld-mygame-users--crossfire
  background-image: url(https://static-cdn.rt.ru/sites/default/files/img/mygame/crossfire-bg.jpg)
  @media (-webkit-min-device-pixel-ratio: 2), (min-resolution: 192dpi) {
    background-image: url(https://static-cdn.rt.ru/sites/default/files/img/mygame/crossfire-bg-2x.jpg)
  }
.ld-mygame-users--lostark
  background-image: url(https://static-cdn.rt.ru/sites/default/files/img/mygame/lostark-bg.jpg)
  @media (-webkit-min-device-pixel-ratio: 2), (min-resolution: 192dpi) {
    background-image: url(https://static-cdn.rt.ru/sites/default/files/img/mygame/lostark-bg-2x.jpg)
  }
.ld-mygame-users-item
  width: 300px
  height: 460px
  display: flex
  text-align center
  position: relative;
  overflow: hidden;
  @media(max-width: 1024px)

    min-width: 35vw
    width: 35vw
    height: 411px
    margin-left: 20px;
  @media(max-width: 767px)
    min-width 70vw
    width: 70vw

  &:before
    content ''
    position: absolute;
    left: 0
    right: 0
    top: 0
    bottom: 0
    background-color: rgba(255,255,255,0.15)
    border-radius 8px
    transition-timing-function ease-in-out
    transition-duration 0.5s
    transition-property opacity, visibility
  &:after
    content ''
    position: absolute;
    left: 0
    right: 0
    top: 0
    bottom: 0
    background-color: #FFFFFF
    border-radius 8px
    mix-blend-mode: soft-light;
    opacity: 0
    transition-timing-function ease-in-out
    transition-duration 0.5s
    transition-property opacity, mix-blend-mode

  
  

@keyframes hideLogo
  0%
    top: 55px
    opacity: 0

  100%
    opacity:0.7
    top: 0
@keyframes showLogo
  0%
    top: 58%
    opacity: 0
  70%
    top: 58%
    opacity: 0
  100%
    opacity: 1
    top: 58%

@keyframes showButton
  0%

    opacity: 0
  50%
    opacity: 0
  100%
    opacity: 1

@keyframes showUser-warface
  0%
    width 107%
    bottom: 37%
    opacity: 0
    width 107%
    bottom: 32%
    left: -6%
    right: -1%
    overflow visible
  40%
    opacity: 0
  100%
    opacity: 1
    overflow hidden
    width 107%
    bottom: 37%
    left: -6%
    right: -1%
@keyframes showUser-lostark
  0%
    opacity: 0
    width 102%
    right: 10px
    bottom: 32%
    overflow visible
  40%
    opacity: 0
  100%
    opacity: 1
    overflow hidden
    width 102%
    bottom: 37%
    right: 2%
@keyframes showUser-archeage
  0%
    opacity: 0
    bottom: 32%
    overflow visible

    width 103%
    right: 4%
  40%
    opacity: 0
  100%
    opacity: 1
    overflow hidden
    width 103%
    bottom: 37%
    right: 4%
@keyframes showUser-crossfire
  0%
    opacity: 0
    width 105%
    right: -3%
    left: -5%
    bottom: 32%
    overflow visible
  40%
    opacity: 0
  100%
    opacity: 1
    overflow hidden
    width 105%
    bottom: 37%
    right: -3%
    left: -5%

@keyframes hideUser
  0%
    bottom: -25px
    opacity 0
  40%
    opacity: 0
  100%
    bottom: -30px
    opacity 0.7


.ld-mygame-users-img-tag
  opacity: 1
  position: absolute;
.ld-mygame-users-img-tag-hover
    opacity: 0

.ld-mygame-users-item--warface.ld-mygame-users-item--active,
.ld-mygame-users-item--warface.ld-mygame-users-item:hover
  .ld-mygame-users-img
    opacity: 1
    width 107%
    bottom: 37%
    left: -6%
    right: -1%
    border-radius 65%
    animation showUser-warface  1s ease-in-out
.ld-mygame-users-item--lostark.ld-mygame-users-item--active,
.ld-mygame-users-item--lostark.ld-mygame-users-item:hover
  .ld-mygame-users-img
    opacity: 1
    bottom: 37%
    border-radius 65%
    width 102%
    bottom: 37%
    right: 2%
    left: -2%
    animation showUser-lostark  1s ease-in-out
.ld-mygame-users-item--archeage.ld-mygame-users-item--active,
.ld-mygame-users-item--archeage.ld-mygame-users-item:hover
  .ld-mygame-users-img
    opacity: 1
    bottom: 37%
    border-radius 65%
    width 103%
    bottom: 37%
    right: 4%
    animation showUser-archeage  1s ease-in-out
.ld-mygame-users-item--lostark.ld-mygame-users-item--active,
.ld-mygame-users-item--lostark.ld-mygame-users-item:hover
  .ld-mygame-users-img
    left: -20px
.ld-mygame-users-item.ld-mygame-users-item--active,
.ld-mygame-users-item.ld-mygame-users-item:hover
  .ld-mygame-users-img-tag
    opacity: 0
  .ld-mygame-users-img-tag-hover
    opacity: 1


.ld-mygame-users-item--crossfire.ld-mygame-users-item--active,
.ld-mygame-users-item--crossfire.ld-mygame-users-item:hover
  .ld-mygame-users-img
    opacity: 1
    bottom: 37%
    border-radius 65%
    width 105%
    right: -3%
    left: -5%
    animation showUser-crossfire  1s ease-in-out
.ld-mygame-users-item--active,
.ld-mygame-users-item:hover
  overflow: visible;
  &:before
    opacity: 0
  &:after
    opacity: 1
  .ld-mygame-users-logo
    opacity: 1
    top: 58%
    animation showLogo  1s ease-in-out
  .ld-mygame-users-img
    opacity: 1
    width 305px
    bottom: 37%
    right: 10px
    border-radius 65%
    animation showUser  1s ease-in-out

  .ld-mygame-users-button
    opacity: 1
    animation showButton  1s ease-in-out
  .ld-mygame-users-img .rt-img-container
    margin-bottom: 0
  .ld-mygame-users-content

    &:before
      opacity: 1
    &:after
      opacity: 1
.ld-mygame-users-scroll
  max-width calc(100% + 20px)
  width calc(100% + 20px)
  overflow-x auto
  overflow-y: hidden
  position: relative;
  left: -20px
  padding-left: 20px
  &:after
    content ''
    @media(max-width: 1024px)

      min-width: 20px
      height: 20px
      display: block
.ld-mygame-users-content
  padding-top: 51px
  max-width 100%
  position: relative;
  
  &:after
    content ''
    position: absolute;
    left: 0
    right: 0
    top: 0
    bottom: 0
    opacity: 0
    background-color: rgba(13, 22, 45, 0.5);
    border-radius 8px
    mix-blend-mode: soft-light;
    border-radius 8px
    transition-timing-function ease-in-out
    transition-duration 0.5s
    transition-property opacity
    
  &:before
    content ''
    position: absolute;
    top: 0
    left: 0
    bottom: 0
    right: 0
    mix-blend-mode: overlay;
    border: 3px solid #FFFFFF;
    box-sizing: border-box;
    border-radius: 8px;
    opacity: 0
    transition-timing-function ease-in-out
    transition-duration 0.5s
    transition-property opacity, visibility
.ld-mygame-users-logo
  position: absolute;
  display: inline-block
  width: 100%
  height: 63px
  text-align: center
  position: relative;
  top: 0
  z-index 2
  opacity:0.7
  animation hideLogo  1s ease-in-out

  .rt-img
    max-height 100%
    display: inline-block

.ld-mygame-users-img
  z-index 2
  width: 100%
  overflow-y hidden
  bottom: -30px
  position: absolute;
  left: 0
  opacity: 0.7
  right: 0
  border-radius 0
  animation hideUser 1s ease-in-out
  .rt-img-container
    margin-bottom: -35px
    transition-timing-function ease-in-out
    transition-duration 0.5s
    transition-property margin
  .rt-img
    max-width 100%

.ld-mygame-users-button
  position: absolute;
  bottom: 28px
  left: 50px
  right: 50px
  display: block
  min-width: calc(100% - 100px)
  opacity: 0


